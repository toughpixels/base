body{margin:0}*,*::before,*::after{box-sizing:border-box}textarea{resize:vertical}table{border-collapse:collapse}img,svg,video,canvas,audio,iframe,embed,object{display:block;vertical-align:middle}img,video{max-width:100%;height:auto}html{scroll-behavior:smooth}.sr-only{position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);white-space:nowrap;border-width:0}.px-sm{padding:0 var(--small-gap)}.p-sm{padding:var(--small-gap)}.py-sm{padding:var(--small-gap)0}@media screen and (--sm){.py-sm{padding:var(--large-gap)0}}:root{--small-gap:1rem;--medium-gap:2rem}.mx-auto{margin-right:auto;margin-left:auto}.space-y-sm>*+*{margin-top:var(--small-gap)}body{font-size:1.2rem}h1{font-size:3.5rem}h2{font-size:3rem}h3{font-size:2.5rem}h4{font-size:2rem}h5{font-size:1.5rem}h6{font-size:1.25rem}.tc{text-align:center}.flex{display:flex}.flex-1{flex:1}.direction-column{flex-direction:column}.align-start{align-items:flex-start}.align-center{align-items:center}.align-end{align-items:flex-end}.justify-start{justify-content:flex-start}.justify-center{justify-content:center}.justify-between{justify-content:space-between}.justify-end{justify-content:flex-end}.justify-around{justify-content:space-around}@media screen and (max-width:var(--sm)){.row-reverse-sm{flex-direction:row-reverse}}